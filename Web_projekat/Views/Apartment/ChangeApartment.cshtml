                    var str = "list[" + els + "].Key";
                    var strr = "list[" + els + "].Value";
                    $('#datesmaindiv').append('<div id="datesdiv" class="dates"><input type="text" name=' + str + ' autocomplete="off" style="width:150px" id="user1" class="inputdate" placeholder="dd-mm-yyyy" /> <input type="text" name=' + strr + ' autocomplete="off" style="width:150px" id="user1" class="inputdate" placeholder="dd-mm-yyyy" /> <button type="button" id="datedelete' + els + '">Delete</button> </div>');
                    $('.dates #user1').datepicker({
                        'format': 'dd-mm-yyyy',
                        'autoclose': true
                    });
                    $('#datedelete' + els).on("click", function () {
                        $(this).parent().remove();
                    });
                });
            });
