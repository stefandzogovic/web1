                        <input type="text" name="list[@cnt].Key" value=@apartment.Times[cnt1] autocomplete="off" style="width:150px" id="user1" class="inputdate" placeholder="dd-mm-yyyy" />
                        @{
                            int tempcnt = cnt1 + 1;
                        }
                        <input type="text" name="list[@cnt].Value" value=@apartment.Times[tempcnt] autocomplete="off" style="width:150px" id="user1" class="inputdate" placeholder="dd-mm-yyyy" />
